#include "Point.h"

Point::Point(double x, double y, double h){
	this->x = x;
	this->y = y;
	this->h = h;	
}

double Point::getX(){
	return this->x;
}

double Point::getY(){
	return this->y;
}

double Point::getH(){
	return this->h;
}

void Point::setX(double x){
	this->x = x;
}

void Point::setY(double y){
	this->y = y;
}

void Point::setH(double h){
	this->h = h;
}
